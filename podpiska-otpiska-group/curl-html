<script>jQuery(function ($) {
    {
        var loadMore = $('#tag-items-list-container').loadMore({
            url: '/en/tag/restaurant-reviews/items',
            errorCallback: function(errCode) {
            },
        });
        
        function reloadList() {
            loadMore.sendRequest(true, function() {});                              
        }
        
        function toggleSorts() {
            if ($('#peerType-channel').is(':checked')) {
                $('#channels-sorts').removeClass('d-none').addClass('d-block');
                $('#chats-sorts').removeClass('d-block').addClass('d-none');
            } 
            else if ($('#peerType-chat').is(':checked')) {
                $('#channels-sorts').removeClass('d-block').addClass('d-none');
                $('#chats-sorts').removeClass('d-none').addClass('d-block');
            }
        }
        
        $('#tag-items-list-container').on('change', '.form-filter-js', function(e) {
            toggleSorts();
            reloadList();
        });
        
        $('#categoryid, #sortchannel, #sortchat').on('change', function(e) {
            reloadList();
        });
        
        toggleSorts();
    };
    new TopSearch({
        requestUrl: '/en/channels/global-search'
    });
});</script>    <script type="text/javascript">

